#include <stdio.h>
int main() {
    int arr[100],size,count=0,n;
    printf("what is the size of array\n");
    scanf("%d",&size);
    for(int i=0; i<size;i++){
    printf("enter a number to store in array\n");
    scanf("%d",&arr[i]);
}
    printf("enter the number whose frequency you want\n");
    scanf("%d",&n);
    for(int i=0;i<size;i++){
      
        if(arr[i]==n){
            count+=1;
        }
    }
    printf("frequency of %d in array is %d",n,count);
    return 0;
}

