#include <stdio.h>
int main() {
    int arr[100],size,max1,max2;
    printf("what is the size of array\n");
    scanf("%d",&size);
    for(int i=0; i<size;i++){
    printf("enter a number to store in array\n");
    scanf("%d",&arr[i]);
}
    if(arr[0]>arr[1]){
        max1=arr[0];
        max2=arr[1];
    }
    else{
        max1=arr[1];
        max2=arr[0];
    }
    for(int i=2;i<size;i++){
        if(arr[i]>max1){
            max2=max1;
            max1=arr[i];
        }
        else if(arr[i]>max2 && arr[i]!=max1){
            max2=arr[i];
        }
    }
    printf("second largest integer in list of integers is %d",max2);
    return 0;
}
