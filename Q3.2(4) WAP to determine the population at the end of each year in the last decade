
#include<stdio.h>
int increment(int current_population){
    int incremented;
    incremented = incremented + incremented*0.1;
    printf("%d\n",incremented);
}
int main()
{int population = 100000;
    for(int year=1;year<11;year++){
        population = population + (population*0.1);
        printf("%d %d\n",year,population);
    }
  

    return 0;
}
